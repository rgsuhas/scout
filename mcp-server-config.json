{
  "mcpServers": {
    "pathfinder-roadmap-service": {
      "command": "python",
      "args": [
        "-m",
        "src.mcp_server"
      ],
      "cwd": "/home/rg/gbl/scout",
      "env": {
        "PYTHONPATH": "/home/rg/gbl/scout",
        "AI_PROVIDER": "google",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
        "GOOGLE_MODEL": "gemini-2.5-flash",
        "AI_MAX_TOKENS": "65536",
        "AI_TEMPERATURE": "0.7",
        "AI_TIMEOUT": "60",
        "LOG_LEVEL": "info",
        "SERVICE_VERSION": "1.0.0"
      },
      "description": "AI-powered roadmap generation and update service"
    }
  }
}
